{
  "name": "vue-kuzzle",
  "version": "1.0.0-beta.2",
  "description": "Use Kuzzle with Vue.js",
  "main": "dist/vue-kuzzle.umd.js",
  "module": "dist/vue-kuzzle.esm.js",
  "unpkg": "dist/vue-kuzzle.min.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "yarn build:browser && yarn build:es && yarn build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "prepublishOnly": "yarn test && yarn build",
    "dev": "nodemon --exec 'yarn build:es && yarn build:umd' --watch src",
    "test": "yarn build && yarn test:eslint && yarn test:types && yarn test:e2e",
    "test:eslint": "eslint --ext .js {src,ssr,lib,build}/**/*.js",
    "test:types": "tsc -p types/test",
    "test:e2e": "cd tests/demo && yarn test",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thenikso/vue-kuzzle.git"
  },
  "keywords": [
    "vue",
    "kuzzle"
  ],
  "author": "Nicola Peduzzi <thenikso@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thenikso/vue-kuzzle/issues"
  },
  "homepage": "https://github.com/thenikso/vue-kuzzle#readme",
  "peerDependencies": {
    "kuzzle-sdk": ">=6 <7"
  },
  "dependencies": {
    "throttle-debounce": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "kuzzle-sdk": "6.0.0",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.1",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "typescript": "^3.3.3",
    "uglify-es": "^3.1.6",
    "vue": "^2.6.4",
    "vue-property-decorator": "^7.0.0",
    "vuepress": "^0.14.2"
  }
}
